<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
	<tr>
		<th>ID</th>
		<th>Username</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th></th>
	</tr>
	
	<% for (i in userlist) {%>
		<tr>
			<td><%= userlist[i].id %>  </td>
			<td><%= userlist[i].username %>  </td>
			<td><%= userlist[i].firstName %> </td>
			<td><%= userlist[i].lastName %> </td>
			<td><%= userlist[i].email %></td>
			<td><a href="/users/info/<%= userlist[i].id %>">View</a> </td>
		</tr>
	<%}%>
	
	</table>
	
  </body>
</html>
